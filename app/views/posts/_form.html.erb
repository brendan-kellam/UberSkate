<%= form_for (@post) do |f| %>
    
    <div class="title">
        <%= f.label :title %><br>
        <%= f.text_field :title %>
    </div>
    
    <div class="description">
        <%= f.label :description %><br>
        <%= f.text_area :description %>
    </div>
    
    <div class="lat">
        <%= f.label :lat %><br>
        <%= f.text_field :lat %>
    </div>
    
    <div class="long">
        <%= f.label :long %><br>
        <%= f.text_field :long %>
    </div>
    
    <div class="actions">
        <%= f.submit %>
    </div>

<% end %>